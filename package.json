{
  "name": "aos-graphql",
  "description": "AOS modules for building with graphql. GraphQL Runtime implemented in Lua+C. ao GraphQL Server with aos integration. Arweave Gateway Implementation",
  "author": "Tyler Hall<tyler@hyper.io>",
  "type": "module",
  "main": "repl.js",
  "scripts": {
    "build": "mkdir -p build && cd build && cmake .. && make clean all",
    "dev": "nodemon",
    "preinstall": "npx only-allow npm",
    "prepare": "husky",
    "staged": "lint-staged",
    "start": "node --experimental-wasm-memory64 repl.js"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@permaweb/ao-loader": "^0.0.35",
    "chalk": "^5.3.0",
    "husky": "^9.1.4",
    "i": "^0.3.7",
    "lint-staged": "^15.2.8",
    "lua-json": "^1.0.1",
    "markdown-toc-gen": "^1.0.1",
    "nodemon": "^3.1.4",
    "npm": "^10.8.2",
    "only-allow": "^1.2.1",
    "sort-package-json": "^2.10.0",
    "standard": "^17.1.0"
  }
}
